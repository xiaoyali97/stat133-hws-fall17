#datasets contains most of the database we are going to use
library(datasets)
library(boot)
#graphic
library(ggplot2)
library(ggmosaic)
library(plotly)
head(airquality)
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
guides(colour = guide_legend(reverse = TRUE)) +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
guides(colour = guide_legend(reverse = FALSE)) +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
guides(colour = guide_legend(reverse = TRUE)) +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
guide_legend(reverse = TRUE) +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
guides(col = guide_legend(reverse = TRUE)) +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
guides(col = guide_legend(reverse = FALSE)) +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
scale_fill_manual(values = c('lightblue','darkblue')) +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
scale_fill_manual(low = "#132B43", high = "#56B1F7") +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
scale_fill_gradient(low = "#132B43", high = "#56B1F7") +
ggtitle(label = "Heat Map")
ggplot(airquality, aes(Day, Month)) +
geom_tile(aes(fill = Temp)) +
scale_fill_gradient(high = "#132B43", low = "#56B1F7") +
ggtitle(label = "Heat Map")
